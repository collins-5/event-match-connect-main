{
  "name": "event-match-connect-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start --dev-client",
    "start": "expo start --go",
    "web": "expo start --web",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "codegen": "graphql-codegen --config codegen.yml",
    "get-ip": "node src/lib/scripts/get-ip.js",
    "dev:with-setup": "npm run get-ip && npm run codegen && npm run dev",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "prebuild:android": "npm run codegen && npm run prebuild:clean",
    "build:dev": "eas build --profile development",
    "build:dev:android": "eas build --profile development --platform android",
    "build:preview": "eas build --profile preview",
    "update:packages": "npx expo install --check",
    "build:production": "eas build --platform android --profile production",
    "build:android": "npm run prebuild:clean && cd android && gradlew assembleRelease",
    "update:production": "node src/lib/scripts/update-app-over-the-air.js",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "doctor": "npx expo-doctor --verbose",
    "test": "jest"
  },
